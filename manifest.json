{
  "manifest_version": 2,
  "name": "HOST Lançar Horas",
  "version": "0.3.0",
  "description": "Lançamento de horas para o sistema HOST",
  "icons": {
  	"16": "host.png",
    "48": "host.png",
    "128": "host.png"
  },
  "background": {
    "scripts" : ["background.js"]
  },
  "browser_action": {
    "default_title": "HOST Lançar Horas",
    "default_icon": "host.png",
    "default_popup": "popup.html"
  },
  "web_accessible_resources" : ["fill_hours.js"],
  "permissions": [
    "tabs",
    "activeTab",
    "declarativeContent"
  ]
}